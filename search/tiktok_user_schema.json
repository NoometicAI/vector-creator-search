{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Schema for TikTok user data",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {"type": "integer"},
            "image": {"type": "string", "format": "uri"},
            "name": {"type": "string"},
            "bio": {"type": "string"},
            "url": {"type": "string", "format": "uri"},
            "likes": {"type": "integer"},
            "followers": {"type": "integer"},
            "following": {"type": "integer"},
            "url_bird": {"type": "string", "format": "uri"},
            "platform": {"type": "string"},
            "video_info": {
                "type": "array",
                "items": {"type": "string"}
            },
            "video_urls_bird": {
                "type": "array",
                "items": {"type": "string", "format": "uri"}
            },
            "video_urls_tk": {
                "type": "array",
                "items": {"type": "string", "format": "uri"}
            },
            "video_thumbnails": {
                "type": "array",
                "items": {"type": "string", "format": "uri"}
            },
            "avg_engagement": {"type": "number"},
            "avg_hearts": {"type": "number"},
            "avg_comments": {"type": "number"},
            "engagements": {
                "type": "array",
                "items": {"type": "number"}
            },
            "hearts": {
                "type": "array",
                "items": {"type": "number"}
            },
            "comments": {
                "type": "array",
                "items": {"type": "number"}
            },
            "avg_engagement_c": {"type": "number"},
            "avg_hearts_c": {"type": "number"},
            "avg_comments_c": {"type": "number"},
            "engagements_c": {
                "type": "array",
                "items": {"type": "number"}
            },
            "hearts_c": {
                "type": "array",
                "items": {"type": "number"}
            },
            "comments_c": {
                "type": "array",
                "items": {"type": "number"}
            },
            "last_activity": {"type": "string"},
            "last_activity_datetime": {"type": "string", "format": "date-time"},
            "sorted_time_posted": {
                "type": "array",
                "items": {"type": "string"}
            },
            "timestamp": {"type": "string", "format": "date-time"},
            "country": {"type": "string"},
            "earnings_post": {"type": "string"},
            "keys": {"type": "string"},
            "language": {"type": "string"},
            "age_raw": {"type": ["null", "number"]},
            "gender_raw": {"type": ["null", "string"]},
            "age": {"type": ["null", "number"]},
            "gender": {"type": ["null", "string"]},
            "audience_genders_per_age": {
                "type": "object",
                "patternProperties": {
                    "^[0-9-]+$": {"type": "number"}
                }
            }
        },
        "required": ["id", "name", "url", "platform"]
    }
}